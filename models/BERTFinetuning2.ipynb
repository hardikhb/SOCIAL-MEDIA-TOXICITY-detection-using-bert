    "from torch import nn\n",
    "from torch.nn import functional as F\n",
    "from transformers import BertTokenizer, BertForSequenceClassification, AdamW\n",
    "\n",
    "# Define your model architecture\n",
    "class BERT_architecture(nn.Module):\n",
    "    def __init__(self, bert_model):\n",
    "        super(BERT_architecture, self).__init__()\n",
    "        self.bert = bert_model\n",
    "        self.classifier = nn.Linear(bert_model.config.hidden_size, 2)  # Assuming 2 classes for classification\n",
    "\n",
    "    def forward(self, input_ids, attention_mask):\n",
    "        output = self.bert(input_ids, attention_mask=attention_mask)\n",
    "        logits = output.logits  # Access the logits from the SequenceClassifierOutput\n",
    "        return logits\n",
    "\n",
    "# Initialize tokenizer and model\n",
    "tokenizer = BertTokenizer.from_pretrained('bert-base-uncased')\n",
    "bert = BertForSequenceClassification.from_pretrained('bert-base-uncased')\n",
    "model = BERT_architecture(bert)\n",
    "\n",
    "device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
    "model = model.to(device)\n",
    "\n",
    "optimizer = AdamW(model.parameters(), lr=1e-5)\n",
    "\n",
    "# Example data (you will replace this with your actual dataset)\n",
    "sentences = [\"This is a positive sentence.\", \"This is a negative sentence.\"]\n",
    "labels = torch.tensor([1, 0]).to(device)  # Example binary labels (1 for positive, 0 for negative)\n",
    "\n",
    "# Tokenize input sentences\n",
    "inputs = tokenizer(sentences, return_tensors='pt', padding=True, truncation=True, max_length=512)\n",
    "inputs = {key: value.to(device) for key, value in inputs.items()}\n",
    "\n",
    "# Train function\n",
    "def train():\n",
    "    model.train()\n",
    "    total_loss = 0\n",
    "    correct_predictions = 0\n",
    "    \n",
    "    optimizer.zero_grad()\n",
    "    \n",
    "    outputs = model(inputs['input_ids'], inputs['attention_mask'])\n",
    "    logits = outputs  # Extract logits directly\n",
    "    \n",
    "    # Compute loss\n",
    "    loss = F.cross_entropy(logits, labels)\n",
    "    \n",
    "    # Backpropagate and update weights\n",
    "    loss.backward()\n",
    "    optimizer.step()\n",
    "    \n",
    "    total_loss += loss.item()\n",
    "    \n",
    "    # Calculate accuracy\n",
    "    preds = torch.argmax(logits, dim=1)\n",
    "    correct_predictions += (preds == labels).sum().item()\n",
    "    \n",
    "    accuracy = correct_predictions / len(labels)\n",
    "    \n",
    "    return total_loss / len(labels), accuracy\n",
    "\n",
    "# Evaluate function\n",
    "def evaluate():\n",
    "    model.eval()\n",
    "    total_loss = 0\n",
    "    correct_predictions = 0\n",
    "    \n",
    "    with torch.no_grad():\n",
    "        outputs = model(inputs['input_ids'], inputs['attention_mask'])\n",
    "        logits = outputs  # Extract logits directly\n",
    "        \n",
    "        loss = F.cross_entropy(logits, labels)\n",
    "        total_loss += loss.item()\n",
    "        \n",
    "        preds = torch.argmax(logits, dim=1)\n",
    "        correct_predictions += (preds == labels).sum().item()\n",
    "    \n",
    "    accuracy = correct_predictions / len(labels)\n",
    "    \n",
    "    return total_loss / len(labels), accuracy\n",
    "\n",
    "# Initialize the best validation loss as infinity\n",
    "best_valid_loss = float('inf')\n",
    "\n",
    "# Empty lists to store training and validation loss of each epoch\n",
    "train_losses = []\n",
    "valid_losses = []\n",
    "\n",
    "# Number of epochs\n",
    "epochs = 10\n",
    "\n",
    "# For each epoch\n",
    "for epoch in range(epochs):\n",
    "    print(f'\\nEpoch {epoch + 1} / {epochs}')\n",
    "    \n",
    "    # Train the model\n",
    "    train_loss, train_accuracy = train()\n",
    "    \n",
    "    # Evaluate the model\n",
    "    valid_loss, valid_accuracy = evaluate()\n",
    "    \n",
    "    # Save the best model based on validation loss\n",
    "    if valid_loss < best_valid_loss:\n",
    "        best_valid_loss = valid_loss\n",
    "        torch.save(model.state_dict(), 'saved_weights.pt')\n",
    "    \n",
    "    # Append training and validation loss\n",
    "    train_losses.append(train_loss)\n",
    "    valid_losses.append(valid_loss)\n",
    "    \n",
    "       \n",
    "    print('\\nTraining Loss: {}'.format(train_loss))\n",
    "    print('Validation Loss: {}'.format(valid_loss))\n"
   ]
